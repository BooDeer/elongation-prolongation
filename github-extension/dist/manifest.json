{
	"name": "GithubMask",
	"description": "No more spam from followers",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
	},
	"permissions": ["storage", "scripting"],
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": [
				"https://github.com/"
			],
			"js": ["github.js"]
		}
	]
}